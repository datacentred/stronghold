.container
  %h1.go-back
    %a{href: 'javascript:history.back()'}
      %i.fa.fa-area-chart
      = @project.organization.name
  %p
    %strong Project:
    = @project.name
  %p
    %strong From:
    = @from_date
  %p
    %strong To:
    = @to_date

  %p
    %strong Total:
    == Â£#{number_with_precision(@grand_total, :delimiter => ',', :separator => '.', :precision => 2)}

  = render partial: 'admin/usage/results/vouchers',  locals: { vouchers: @active_vouchers }
  = render partial: 'admin/usage/results/instances', locals: { project: @project, instances:         @instance_usage            }
  = render partial: 'admin/usage/results/volumes',   locals: { project: @project, volumes:           @volume_usage              }
  = render partial: 'admin/usage/results/images',    locals: { project: @project, images:            @image_usage               }
  = render partial: 'admin/usage/results/objects',   locals: { project: @project, tbh:               @object_storage_usage      }
  = render partial: 'admin/usage/results/ips',       locals: { ip_quotas:         @ip_quota_usage,                                                           external_gateways: @external_gateway_results,
                                                               project:           @project                     }
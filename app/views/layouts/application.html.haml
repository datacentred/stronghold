!!!
%html
  %head
  %title Datacentred Dashboard
  = favicon_link_tag 'favicon.ico'
  = stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true
  = javascript_include_tag 'application', 'data-turbolinks-track' => true
  = csrf_meta_tags
  %meta{'charset' => "utf-8"}
  %meta{'http-equiv' => 'X-UA-Compatible', 'content' => 'IE=edge'}
  %meta{'name' => 'viewport', 'content' => 'width=device-width, initial-scale=1'}

%body
  #wrap

    .container
      %a{href: support_root_path, class: 'back-to-dash' }
        = image_tag 'logo_small.png', class: 'logo'
        %div
          %h2 > Dashboard
      #flashes= display_flash
      %nav= render partial: 'shared/support_nav'
      = yield
      .push
  /= render partial: 'shared/footer'
  :javascript
    (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r;
        i[r] = i[r] || function () {
            (i[r].q = i[r].q || []).push(arguments)
        }, i[r].l = 1 * new Date();
        a = s.createElement(o),
        m = s.getElementsByTagName(o)[0];
        a.async = 1;
        a.src = g;
        m.parentNode.insertBefore(a, m)

    })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

    ga('create', 'UA-42114405-1', 'datacentred.co.uk');
    ga('send', 'pageview');
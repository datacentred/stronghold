%table.table{:'ng-controller' => "TicketsController"}
  %thead
    %th.status-head
    %th
      Ticket
  %tbody{:'ng-repeat' => ("ticket in tickets['" + status + "']")}
    %tr
      %td.status
        %i.fa.fa-circle{:class => ("status-" + status)}
      %td
        {{ticket.attrs.fields.summary}}